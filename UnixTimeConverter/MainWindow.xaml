<Window x:Class="UnixTimeConverter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnixTimeConverter"
        mc:Ignorable="d"
        Title="UnixTime Converter" Height="450" Width="800"
        Background="#121212"
        WindowStyle="None"
        ResizeMode="CanResizeWithGrip"
        AllowsTransparency="True">
    <StackPanel>
        <Canvas>
            <Image Canvas.Right="5" Canvas.Top="5" Source="./assets/close-icon.png" Width="25" Height="25" MouseUp="UIElement_OnMouseUp" MouseEnter="UIElement_OnMouseEnter" />
        </Canvas>
    </StackPanel>
</Window>
